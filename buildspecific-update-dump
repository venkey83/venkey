export DATE=`date +%Y%m%d%H%M`
  aws rds create-db-snapshot --db-instance-identifier venkey --region ap-southeast-1 --db-snapshot-identifier venkey-$DATE
  sleep 300s
  export DBIDENTIFIER=venkey.czrrfh9ifhyq.ap-southeast-1.rds.amazonaws.com-$DATE
  echo $DBIDENTIFIER
  aws rds restore-db-instance-from-db-snapshot  --db-instance-identifier venkey-$DATE --region ap-southeast-1 --db-snapshot-identifier venkey-$DATE --db-instance-class db.t3.small > rdsoutput.txt
  echo "Temp db ready"
  export ENDPOINT=venkey-$DATE.czrrfh9ifhyq.ap-southeast-1.rds.amazonaws.com
  echo $ENDPOINT
